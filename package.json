{
  "name": "level-5-capstone",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/index.js",
  "mainHTML": "dist/index.html",
  "engines": {
    "node": "18.14.0"
  },
  "scripts": {
    "start-netlify": "netlify dev",
    "start": "nodemon ./src/index.ts",
    "start-build": "nodemon ./build/src/index.js",
    "build-start-netlify": "npm run build-netlify && npm run start-netlify",
    "server": "netlify dev",
    "dist": "nodemon ./dist/index.js",
    "upload": "git push github main",
    "deploy": "netlify deploy",
    "staging": "netlify deploy",
    "production": "netlify deploy --prod",
    "migration": "knex migrate:latest --env cockroachdb",
    "postmigration": "knex seed:run --env cockroachdb",
    "seed": "knex seed:run --env cockroachdb",
    "rollback": "knex migrate:rollback --env cockroachdb",
    "tables-mysql": "knex migrate:latest --env mysql",
    "posttables-mysql": "knex seed:run --env mysql",
    "migration-mysql": "knex migrate:latest --env mysql",
    "postmigration-mysql": "knex seed:run --env mysql",
    "data-mysql": "knex seed:run --env mysql",
    "seed-mysql": "knex seed:run --env mysql",
    "rollback-mysql": "knex migrate:rollback --env mysql",
    "migration-elephantsql": "knex migrate:latest --env elephantsql",
    "postmigration-elephantsql": "knex seed:run --env elephantsql",
    "seed-elephantsql": "knex seed:run --env elephantsql",
    "rollback-elephantsql": "knex migrate:rollback --env elephantsql",
    "migration-typeorm": "typeorm-ts-node-commonjs migration:run --dataSource data-source.ts",
    "rollback-typeorm": "typeorm-ts-node-commonjs migration:revert --dataSource data-source.ts",
    "compile": "tsc",
    "compile-minify": "webpack --config webpack.config.minify.js",
    "build": "npm run minify",
    "build-netlify": "netlify build",
    "minify": "node ./minify.js build",
    "webpack": "webpack --config webpack.config.dist.js",
    "test": "jest",
    "watch": "jest --watchAll",
    "rebuild": "cd .. && npm run webpack && cd dist && npm start"
  },
  "keywords": [],
