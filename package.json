{
  "name": "level-5-capstone",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "mainHTML": "src/index.html",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "dist": "nodemon ./dist/index.js",
    "server": "nodemon ./src/index.ts",
    "upload": "git push github main",
    "deploy": "git push heroku main",
    "tables": "knex migrate:latest --env mysql",
    "posttables": "knex seed:run --env mysql",
    "migration": "knex migrate:latest --env mysql",
    "postmigration": "knex seed:run --env mysql",
    "data": "knex seed:run --env mysql",
    "seed": "knex seed:run --env mysql",
    "rollback": "knex migrate:rollback --env mysql",
    "migration-elephantsql": "knex migrate:latest --env elephantsql",
    "postmigration-elephantsql": "knex seed:run --env elephantsql",
    "seed-elephantsql": "knex seed:run --env elephantsql",
    "rollback-elephantsql": "knex migrate:rollback --env elephantsql",
    "migration-typeorm": "typeorm-ts-node-commonjs migration:run --dataSource data-source.ts",
    "rollback-typeorm": "typeorm-ts-node-commonjs migration:revert --dataSource data-source.ts",
    "build": "rm build -rf && tsc && node dist.js",
    "webpack": "webpack --config webpack.config.dist.js",
    "test": "jest",
    "watch": "jest --watchAll",
    "rebuild": "cd .. && npm run webpack && cd dist && npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
