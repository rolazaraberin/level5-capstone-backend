    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.7.16",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "jest": "^28.1.3",
    "terser": "^5.16.3",
